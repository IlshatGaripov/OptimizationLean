{
  // MODE
  "optimization-mode": "BruteForce",
  "execution-mode": "Azure",
  "max-threads": 100,
  "fitness-score": "SharpeRatio",

  // FITNESS FILTER
  "fitness-filter": {
    "min-trades": 9,
    "max-drawdown": 0.35
  },

  // THE DATES
  "startDate": "2017-01-02",
  "endDate": "2018-01-02",

  // WALK FORWARD
  "walk-forward": {
    "enabled": true,
    "anchored": false,
    "in-sample-period": 365, // in days
    "step": 60
  },

  // GENES
  "genes": [
    {
      "key": "period",
      "min": 60,
      "max": 120,
      "step": 60
    },
    {
      "key": "mult",
      "min": 1.8,
      "max": 2.5,
      "step": 0.3
    }
  ],

  // LEAN
  "algorithm-type-name": "SuperTrendAlgorithm",
  "algorithm-location": "../../../Optimization.Example/bin/debug/Optimization.Example.dll",
  "data-folder": "C:/Users/sterling/Google Drive/Data",

  // AZURE
  "batch-account-name": "",
  "batch-account-key": "",
  "batch-account-url": "",
  "storage-account-name": "",
  "storage-account-key": "",
  "dedicated-nodes": 0,
  "low-priority-nodes": 2,

  // GENETIC OPTIMIZATION
  "population-size": 24,
  "generations": 20,
  "stagnation-generations": 30,
  "mutation-probability": 0.5,
  "crossover-probability": 0.75
}