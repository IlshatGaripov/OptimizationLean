{
  // MODE
  "optimization-mode": "BruteForce",
  "execution-mode": "Azure",
  "max-threads": 100,
  "fitness-score": "SharpeRatio",

  // FITENSS FILTER
  "fitness-filter": {
    "min-trades": 9,
    "max-drawdown": 0.35
  },

